{
    "name": "graphene-pk11",
    "version": "2.0.10",
    "description": "A simple layer for interacting with PKCS #11 / PKCS11 / CryptoKI for Node in TypeScript",
    "main": "./build/graphene.js",
    "scripts": {
        "install": "npm run build",
        "build": "typings install && tsc",
        "test": "mocha",
        "pub": "npm run build && node scripts/publish",
        "sync": "git ac && git pull --rebase && git push"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PeculiarVentures/graphene.git"
    },
    "engines": {
        "node": "^4.0.0"
    },
    "bin": {
        "graphene": "./build/console/console.js"
    },
    "dependencies": {
        "pkcs11js": "^1.0.0"
    },
    "devDependencies": {
        "live-server": "^1",
        "mocha": "^3",
        "npm-run-all": "^3",
        "nyc": "^8",
        "typescript": "^1.8.10",
        "typings": "^1.3.3",
        "watch": "^0"
        },
    "bugs": {
        "url": "https://github.com/PeculiarVentures/graphene/issues"
    },
    "keywords": [
        "pkcs11",
        "cryptography",
        "cryptoki",
        "token",
        "smartcard",
        "hsm",
        "bitcoin",
        "RSA",
        "ECC"
    ],
    "author": "PeculiarVentures",
    "license": "MIT"
}
